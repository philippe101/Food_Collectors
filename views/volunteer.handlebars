<!--NAVBAR-->
<nav class="navbar navbar-light col align-items-end fixed-top" style="background-color: #00C4C2;">
    <blockquote class="blockquote">
        <p class="mb-0"><em>“You have not lived today until you have done something for someone who can never repay you.”</em></p>
        <footer class="blockquote-footer text-white">John Bunyan</footer>
    </blockquote>
    <a class="btn btn-outline-dark mb-3 " style="background-color: DarkSalmon;" href="/" role="button">Home</a>
</nav>
<br><br><br>
<!--Main-->
<div class="volunteer-container">
  <div class="row" id="welcome">
    {{!-- {{#each users}} --}}
      <h1 class="text-center text-white py-5">
      <display-1>Welcome {{name}}!</display-1>
      </h1>
    {{!-- {{/each}} --}}
  </div>
    <!--CARD INFO FOR PICKUPS-->
    <div class="row" id="card">
      {{#each orders}}
        <div class="col-md-3 mb-4">
          <div class="card mx-3">
            <div class="card-body">
                <h4 class="card-title">{{this.name}}</h4>
                <p class="card-text">There is {{this.quantity}} of {{this.name}}!</p>
                <p class="card-text"> It expires on <mark>{{this.expiration}}</mark>. 
                <p class="card-text">Let's go get it!</p>
                <p class="card-text"><small class="text-muted">Recieved: {{this.status}}</small></p>
            </div>
            <div class="btn-group mx-auto mb-2" role="group" aria-label="Basic example">
              <input class="btn btn-success" type="submit" value="Picked Up">
              <input class="btn btn-warning" type="submit" value="Delivered">
            </div>
            {{!-- Google Maps Div --}}
              <img class="card-img-bottom" src="http://images.indianexpress.com/2016/11/hazaribagh-759.jpg" alt="Card image cap">
          </div>
        </div>
      {{/each}}
    </div>

</div>
<br><br>
